/*Scroll bar for Breadcrumb*/
#breadcrumbsArea QScrollBar:horizontal
{
    border: 0px solid #eee;
    height: 3px;
    margin: 0;
    padding:0;
}

#breadcrumbsArea  QScrollBar::handle:horizontal {
      background: white;
      min-width: 4px;
  }
#breadcrumbsArea  QScrollBar::add-line:horizontal {
      border: 2px solid grey;
      background: #32CC99;
      width: 0px;
      subcontrol-position: right;
      subcontrol-origin: margin;
  }

 #breadcrumbsArea QScrollBar::sub-line:horizontal {
      border: 2px solid grey;
      background: #32CC99;
      width: 0px;
      subcontrol-position: left;
      subcontrol-origin: margin;
  }
  
  
  
