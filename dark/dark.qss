/* Customize the menu bar. */
QMenuBar{
    border-top-right-radius: 0px;
    border-top-left-radius: 0px;
    border: 0px solid #7d8389;
    padding:0;
    margin: 0px;
}
QMenuBar::item
{
	border:0;
	padding:3px 6px;
	margin:0;
}
QMenuBar::item:selected{
 	background: #4797F1;
    border: 1px solid transparent;
    color: #fff;
	
	color: #ddd;
	border: 1px solid #4C5753;
    background: #424C49;
}
QMenuBar::item:pressed{
    background: #2F3734;
    border: 1px solid #97999d;
    border-bottom-color: transparent;
}
QToolBar#mainToolBar{
/*    background: #eef1f8;*/
/*    background-image: url(:/themes/white/white/bg.png) top left repeat;*/    background: transparent;
    border-bottom-right-radius: 0px;
    border-bottom-left-radius: 0px;
    border: 0px solid #7d8389;
    border: none;
    spacing: 0px;
/* spacing between items in the tool bar */    padding: 0px;
    margin: 0px;
    margin-top: 0px;
}
QToolBar{
    color: #d7d8d9;
    border: none;
}
QToolBar QToolButton:hover:enabled{
/* when selected using mouse or keyboard */    background: qlineargradient(spread:reflect, x1:0.45241, y1:1, x2:0.441, y2:0.505, stop:0.255526 #e6e9eb, stop:0.960212 #d6d9de);
    border: 1px solid #a0a5ab;
    border-radius: 0px;
    padding: 0px;
    background: qlineargradient(spread:pad, x1:0.453399, y1:0, x2:0.462, y2:1, stop:0 #f1f1f2, stop:1 #e7e8e8);
    border: 1px solid #97999d;
}
QToolBar QToolButton:pressed{
    background: qlineargradient(spread:pad, x1:0, y1:0.028, x2:0, y2:0.693, stop:0 #a6aab2, stop:0.249451 #b7bbbf, stop:1 #b7bbbf);
    border: 1px solid #97999d;
    border-radius: 0px;
    padding: 0px;
    color: #fff;
}
QToolBar QToolBar QToolButton:on:active{
    background: qlineargradient(spread:reflect, x1:0.45241, y1:1, x2:0.441, y2:0.505, stop:0.255526 #fff, stop:0.960212 #eee);
    border: 1px solid #999;
}
QToolBar QToolButton:on{
    background: qlineargradient(spread:pad, x1:0, y1:0.028, x2:0, y2:0.693, stop:0 #a6aab2, stop:0.249451 #b7bbbf, stop:1 #b7bbbf);
    border: 1px solid #97999d;
    border-radius: 0px;
    padding: 0px;
}
