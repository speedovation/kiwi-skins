	border-top-width:3px;
}

QTabBar::tab:top:selected
{
    color: #fff;
    border: 1px solid #282C34;
    border-top-color: #FFCD22;
	border-top-width:2px;
	border-bottom-color:transparent;
    background: #2A82DA;
	font-weight: bold;
}
QTabBar::tab:top:last
